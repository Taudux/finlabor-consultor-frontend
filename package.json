{
  "name": "finlabor-consultor-frontend",
  "version": "1.3.11",
  "description": "Hello World!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "dotenv -- electron-builder --publish always"
  },
  "author": "Yael Perez",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "electron": "^32.0.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2",
    "form-data": "^4.0.2",
    "jsrsasign": "^11.1.0",
    "node-fetch": "^2.7.0",
    "undici": "^6.21.2",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.tuempresa.miapp",
    "productName": "Consultor de reporte de credito",
    "publish": [
      {
        "provider": "github",
        "owner": "Taudux",
        "repo": "finlabor-consultor-frontend"
      }
    ],
    "extraFiles": [
      {
        "from": "python_scripts",
        "to": "python_scripts",
        "filter": [
          "**/*.py"
        ]
      }
    ],
    "win": {
      "icon": "build/finlabor_logo-removebg-preview.ico",
      "target": "nsis",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    }
  }
}

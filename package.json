{
  "name": "prueba_1",
  "version": "1.2.0",
  "description": "Hello World!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "Yael Perez",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "electron": "^32.0.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2"
  },
  "build": {
    "appId": "com.tuempresa.miapp",
    "productName": "electron_prueba_1",
    "publish": [
      {
        "provider": "github",
        "owner": "YaelPerez",
        "repo": "electron_prueba_1"
      }
    ],
    "extraFiles": [
      {
        "from": "python_scripts",
        "to": "python_scripts",
        "filter": ["**/*.py"]
      }
    ],
    "win": {
      "target": "nsis",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    }
  }
}
